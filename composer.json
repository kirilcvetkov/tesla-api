{
    "name": "kirilcvetkov/tesla-api",
    "description": "A simple Laravel package to help you connect to your Tesla(s).",
    "keywords": [
        "kirilcvetkov",
        "tesla-api",
        "tesla"
    ],
    "homepage": "https://github.com/kirilcvetkov/tesla-api",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kiril Cvetkov",
            "email": "kcvetkov@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "larapack/dd": "^1.1",
        "mailgun/mailgun-php": "^3.5",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "KirilCvetkov\\TeslaApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KirilCvetkov\\TeslaApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
