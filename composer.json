{
    "name": "kirilcvetkov/tesla-api",
    "description": "A simple Laravel package to help you connect to your Tesla(s).",
    "keywords": [
        "kirilcvetkov",
        "tesla-api",
        "tesla"
    ],
    "homepage": "https://github.com/kirilcvetkov/tesla-api",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kiril Cvetkov",
            "email": "kcvetkov@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KirilCvetkov\\TeslaApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KirilCvetkov\\TeslaApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KirilCvetkov\\TeslaApi\\TeslaApiServiceProvider"
            ],
            "aliases": {
                "TeslaApi": "KirilCvetkov\\TeslaApi\\TeslaApiFacade"
            }
        }
    }
}
